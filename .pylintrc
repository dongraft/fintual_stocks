[MASTER]
ignore-paths=.*/migrations
load-plugins=

[MESSAGES CONTROL]
disable=
    bare-except,
    broad-except,
    fixme,
    invalid-name,
    missing-class-docstring,
    missing-function-docstring,
    missing-module-docstring,
    no-member,
    superfluous-parens,
    too-many-ancestors,
    too-many-locals,
    too-few-public-methods,
    too-many-branches,
    too-many-statements,
    duplicate-code,
    too-many-public-methods,
    too-many-nested-blocks,
    too-many-arguments,
    W0223,
    protected-access,
    C0302,
    E1131

[main]
check-quote-consistency=yes
